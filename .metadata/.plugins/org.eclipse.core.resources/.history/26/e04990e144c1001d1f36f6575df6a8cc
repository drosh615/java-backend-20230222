package trainReservation.entity;

//남아있는 좌석
public class Seat {
	private int roomNumber;
	private String sratNumber;
	private boolean seatStatus;
	
	public Seat() { }
	public Seat(int roomNumber, String sratNumber, boolean seatStatus) {
		super();
		this.roomNumber = roomNumber;
		this.sratNumber = sratNumber;
		this.seatStatus = seatStatus;
	}
	public int getRoomNumber() {
		return roomNumber;
	}
	public String getSratNumber() {
		return sratNumber;
	}
	public boolean isSeatStatus() {
		return seatStatus;
	}
	@Override
	public String toString() {
		return "Seat [roomNumber=" + roomNumber + ", sratNumber=" + sratNumber + ", seatStatus=" + seatStatus + "]";
	}
	
	
	
}
