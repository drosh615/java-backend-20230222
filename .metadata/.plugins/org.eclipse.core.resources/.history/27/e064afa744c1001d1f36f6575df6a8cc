package trainReservation.entity;

//남아있는 좌석
public class Seat {
	private int roomNumber;
	private String sratNumber;
	private boolean seatStatus;
	
	public Seat() { }
	public Seat(int roomNumber, String sratNumber, boolean seatStatus) {
		super();
		this.roomNumber = roomNumber;
		this.sratNumber = sratNumber;
		this.seatStatus = seatStatus;
	}
	
	
	
}
